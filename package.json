{
  "name": "recipecrate",
  "version": "1.0.0",
  "description": "Serverless webpack example using Typescript",
  "main": "handler.js",
  "scripts": {
    "refresh": "sls remove && npm run clean && npm install && sls deploy",
    "clean": "rm -rf .serverless && rm -rf node_modules && rm -rf package-lock.json",
    "test": "LOG_LEVEL=NONE node_modules/.bin/mocha -- --file test/setup.ts --timeout 5000 -r ts-node/register ./test/**/*.test.ts",
    "test:watch": "npm run test -- --watch-extensions ts --watch",
    "tslint": "tslint --project ./tsconfig.json",
    "core:test": "npm run test",
    "core:tslint": "npm run tslint",
    "build": "sls webpack",
    "lint": "jshint .",
    "validate": "npm ls"
  },
  "dependencies": {
    "source-map-support": "^0.5.10"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.17",
    "@types/node": "^10.12.18",
    "chai": "^4.2.0",
    "serverless-webpack": "^5.2.0",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0",
    "ts-loader": "^5.3.3",
    "typescript": "^3.2.4",
    "webpack": "^4.29.0"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
