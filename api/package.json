    "@types/node": "^11.13.20",
    "@types/sinon": "^7.0.13",
    "@types/sinon-chai": "^3.2.2",
    "aws-sdk-mock": "^4.5.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^5.16.0",
